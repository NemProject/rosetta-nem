{
    "name": "rosetta-nem",
    "version": "0.0.1",
    "description": "Rosetta implementation of NEM Network",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/NemProject/rosetta-nem.git"
    },
    "bugs": {
        "url": "https://github.com/NemProject/rosetta-nem"
    },
    "author": {
        "email": "fboucquez@gmail.com",
        "name": "Fernando Boucquez"
    },
    "license": "Apache-2.0",
    "engines": {
        "node": ">=12.0.0"
    },
    "keywords": [
        "rosetta",
        "coinbase",
        "nem",
        "typescript"
    ],
    "files": [
        "dist/src"
    ],
    "homepage": "https://github.com/NemProject/rosetta-nem",
    "main": "./dist/src/index.js",
    "typings": "./dist/src/index.d.ts",
    "scripts": {
        "doc": "typedoc --out \"ts-docs/$(npm run version --silent)\" src",
        "build": "npx rimraf dist && tsc",
        "watch": "tsc -w",
        "test": "mocha -r ts-node/register -r tsconfig-paths/register --timeout 600000 --forbid-only \"test/**/*.test.ts\"",
        "test:e2e": "mocha -r ts-node/register -r tsconfig-paths/register --timeout 600000 --forbid-only \"test/**/*.e2e.ts\"",
        "lint": "eslint --cache src/ test/ --ext .ts",
        "lint:fix": "eslint src/ test/ --ext .ts --fix",
        "prettier": "prettier --write ./src ./test ",
        "style:fix": "npm run prettier && npm run lint:fix",
        "version": "echo $npm_package_version",
        "dev": "npx ts-node-dev --prefer-ts true -r tsconfig-paths/register src/RosettaNem.ts --testnet --remote"
    },
    "devDependencies": {
        "@types/chai": "^4.2.21",
        "@types/express": "^4.17.13",
        "@types/mocha": "^9.0.0",
        "@types/node": "^16.7.10",
        "@typescript-eslint/eslint-plugin": "^4.30.0",
        "@typescript-eslint/parser": "^4.30.0",
        "chai": "^4.3.4",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "install": "^0.13.0",
        "mocha": "^9.1.1",
        "mocha-lcov-reporter": "^1.3.0",
        "npm": "^7.21.1",
        "prettier": "^2.3.2",
        "prettier-plugin-organize-imports": "^2.3.3",
        "rimraf": "^3.0.2",
        "ts-node": "^10.2.1",
        "ts-node-dev": "^1.1.8",
        "tsconfig-paths": "^3.11.0",
        "typedoc": "^0.21.9",
        "typescript": "^4.4.2"
    },
    "dependencies": {
        "node-fetch": "^2.6.1",
        "rosetta-sdk-typescript": "^0.0.1-alpha-202108261014"
    }
}
