{
    "name": "rosetta-nem",
    "version": "0.0.1",
    "description": "Rosetta implementation of NEM Network",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/NemProject/rosetta-nem.git"
    },
    "bugs": {
        "url": "https://github.com/NemProject/rosetta-nem"
    },
    "author": {
        "email": "fboucquez@gmail.com",
        "name": "Fernando Boucquez"
    },
    "license": "Apache-2.0",
    "engines": {
        "node": ">=12.0.0"
    },
    "keywords": [
        "rosetta",
        "coinbase",
        "nem",
        "typescript"
    ],
    "files": [
        "dist/src",
        "dist/api.json",
        "src"
    ],
    "homepage": "https://github.com/NemProject/rosetta-nem",
    "main": "./dist/src/index.js",
    "typings": "./dist/src/index.d.ts",
    "scripts": {
        "openapi:bundle": "swagger-cli bundle ./nem-openapi/openapi.yml --outfile dist/nem-openapi.yml --type yaml && swagger-cli validate ./dist/nem-openapi.yml",
        "openapi:gen": "npm run openapi:bundle && GEN_DIR=\"./src/openapi\" && npx rimraf \"$GEN_DIR\"  && npx openapi-generator-cli generate -g \"typescript-fetch\" -o \"$GEN_DIR\" -i \"dist/nem-openapi.yml\" --additional-properties=\"supportsES6=true\" --additional-properties=\"withInterfaces=true\" --additional-properties=\"legacyDiscriminatorBehavior=false\" --additional-properties=\"useSingleRequestParameter=true\" --additional-properties=\"modelPropertyNaming=original\" --additional-properties=\"enumPropertyNaming=original\" --additional-properties=\"typescriptThreePlus=true\" --type-mappings=Error=ModelError && npx rimraf \"$GEN_DIR/.openapi-generator\" \"$GEN_DIR/.gitignore\" \"$GEN_DIR/.openapi-generator-ignore\" \"$GEN_DIR/tsconfig.json\" && npm run prettier:gen",
        "doc": "typedoc --out \"ts-docs/$(npm run version --silent)\" src",
        "build": "npx rimraf dist && tsc",
        "watch": "tsc -w",
        "test": "nyc --reporter=lcov --extension .ts mocha -r ts-node/register -r tsconfig-paths/register --timeout 600000 --forbid-only \"test/**/*.test.ts\"",
        "test:e2e": "nyc --reporter=lcov --extension .ts mocha -r ts-node/register -r tsconfig-paths/register --timeout 600000 --forbid-only \"test/**/*.e2e.ts\"",
        "lint": "eslint --cache src/ test/ --ext .ts",
        "lint:fix": "eslint src/ test/ --ext .ts --fix",
        "prettier:gen": "prettier --write ./src/openapi",
        "prettier": "prettier --write ./src ./test ./nem-openapi",
        "style:fix": "npm run create-index-files && npm run prettier && npm run lint:fix",
        "create-index-files": "cti create ./src -b -n -e commands -i RosettaNem.ts",
        "coveralls-report": "cat ./coverage/lcov.info | coveralls",
        "version": "echo $npm_package_version",
        "dev": "npx ts-node-dev --prefer-ts true -r tsconfig-paths/register src/RosettaNem.ts --testnet --remote"
    },
    "devDependencies": {
        "@openapitools/openapi-generator-cli": "^2.3.10",
        "@types/chai": "^4.2.21",
        "@types/express": "^4.17.13",
        "@types/mocha": "^9.0.0",
        "@types/node": "^16.7.1",
        "@typescript-eslint/eslint-plugin": "^4.29.2",
        "@typescript-eslint/parser": "^4.29.2",
        "chai": "^4.3.4",
        "coveralls": "^3.1.1",
        "create-ts-index": "^1.8.0",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^3.4.1",
        "install": "^0.13.0",
        "mocha": "^9.1.0",
        "mocha-lcov-reporter": "^1.3.0",
        "npm": "^7.21.0",
        "nyc": "^15.1.0",
        "prettier": "^2.3.2",
        "prettier-plugin-organize-imports": "^2.3.3",
        "rimraf": "^3.0.2",
        "swagger-cli": "^4.0.4",
        "ts-node": "^10.2.1",
        "ts-node-dev": "^1.1.8",
        "tsconfig-paths": "^3.10.1",
        "typedoc": "^0.21.6",
        "typescript": "^4.3.5"
    },
    "dependencies": {
        "rosetta-sdk-typescript": "^0.0.1-alpha-202108261014",
        "node-fetch": "^2.6.1"
    }
}
